{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "func-names": 0,
    "import/no-extraneous-dependencies": 0,
    "new-cap": 0,
    "no-console": ["error", { "allow": ["info", "warn", "error"] }],
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }]
  }
}
