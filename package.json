{
  "name": "elm-boilerplate",
  "description": "A boilerplate for elm apps",
  "version": "0.1.0",
  "author": "LikeJasper <will@willclark.tech>",
  "license": "MIT",
  "homepage": "https://github.com/LikeJasper/elm-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LikeJasper/elm-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/LikeJasper/elm-boilerplate/issues"
  },
  "scripts": {
    "serve:test": "./node_modules/.bin/http-server dist -a localhost -p 7357 -c -1",
    "lint": "./node_modules/.bin/eslint ./features",
    "test:e2e": "./node_modules/.bin/cucumber-js ./features --tags @now || true",
    "test:e2e:now": "./node_modules/.bin/cucumber-js ./features || true",
    "test:e2e:single": "npm run serve:test & npm run test:e2e",
    "test:e2e:watch": "npm run serve:test & ./node_modules/.bin/nodemon --watch dist --watch features --ext js,html --exec 'npm run test:e2e:now'",
    "test:elm:single": "FILE=$(mktemp $(uuidgen).js) && elm-make elm-test/TestRunner.elm --output $FILE && node $FILE; RESULT=$?; rm -f $FILE;",
    "test:elm:watch": "./node_modules/.bin/nodemon --watch 'elm/' --watch 'elm-test/' --ext elm --exec 'npm run test:elm:single'",
    "ncu": "ncu"
  },
  "devDependencies": {
    "cucumber": "^1.0.0",
    "elm": "^0.17.0",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "http-server": "^0.9.0",
    "nodemon": "^1.9.2",
    "npm-check-updates": "^2.6.7",
    "zombie": "^4.2.1"
  }
}
